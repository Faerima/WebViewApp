{
  "expo": {
    "name": "WebViewApp",
    "slug": "webviewapp",
    "scheme": "webviewapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8E5C2"
    },
    "updates": { "fallbackToCacheTimeout": 0 },
    "ios": { "supportsTablet": false, "bundleIdentifier": "ch.roemerhof.webviewapp" },
    "android": { "package": "ch.roemerhof.webviewapp" },
    "extra": {
      "startUrl": "https://roemerhof.kuriersoft.ch/"
    }
  }
}
