{
  "expo": {
    "name": "WebViewApp",
    "slug": "webviewapp",
    "scheme": "webviewapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8E5C2"
    },
    "updates": { "fallbackToCacheTimeout": 0 },
    "userInterfaceStyle": "light",
    "statusBar": {
      "style": "dark",
      "backgroundColor": "#F8E5C2"
    },
    "ios": { 
      "supportsTablet": false, 
      "bundleIdentifier": "ch.roemerhof.webviewapp",
      "statusBar": {
        "style": "dark"
      }
    },
    "android": { 
      "package": "ch.roemerhof.webviewapp",
      "edgeToEdgeEnabled": false,
      "statusBar": {
        "style": "dark",
        "backgroundColor": "#F8E5C2",
        "translucent": false,
        "hidden": false
      },
      "navigationBar": {
        "backgroundColor": "#F8E5C2",
        "visible": true
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "extra": {
      "startUrl": "https://roemerhof.kuriersoft.ch/"
    }
  }
}
