{
  "expo": {
    "name": "WebViewApp",
    "slug": "webviewapp",
    "scheme": "webviewapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8E5C2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "statusBar": {
      "style": "auto"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ch.webviewapp",
      "statusBar": {
        "style": "auto"
      }
    },
    "android": {
      "package": "ch.webviewapp",
      "statusBar": {
        "style": "auto",
        "translucent": true,
        "backgroundColor": "transparent"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "extra": {
      "startUrl": "https://example.com/",
      "eas": {
        "projectId": "428d07f3-7b48-478d-ad9c-dbd1df57e34a"
      }
    }
  }
}
